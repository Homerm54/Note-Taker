                      <i class="fa fa-eye" aria-hidden="true"></i>
                    </a>
                    <a class="badge badge-light mx-1" href="{{noteURL id}}">
                      <i class="fa fa-pen-square" aria-hidden="true"></i>
                    </a>
                    <a class="badge badge-light mx-1" href="{{deleteURL id}}">
                      <i class="fa fa-times" aria-hidden="true"></i>
                    </a>
                  </div>
                </li>
              {{/each}}
            {{else}}
            <li class="list-group-item">No notes added here, if you feel like you need a note, click on the
              <span class=""><i class="fa fa-plus" aria-hidden="true"></i></span> sign at in the category name!</li>
            <li class="list-group-item"></li>
            {{/if}}
          </ul>
        </div>
        <div class="tab-pane fade" id="todo" role="tabpanel" aria-labelledby="todo-tab">
          <ul class="list-group list-group-flush">
            {{# if todo}}
              {{#each todo}}
                <li class="list-group-item">
                  <div class="container-fluid">
                    {{title}}
                    <a href="{{viewURL id}}" class="badge badge-light mx-1">
                      <i class="fa fa-eye" aria-hidden="true"></i>
                    </a>
                    <a class="badge badge-light mx-1" href="{{noteURL id}}">
                      <i class="fa fa-pen-square" aria-hidden="true"></i>
                    </a>
                    <a class="badge badge-light mx-1" href="{{deleteURL id}}">
                      <i class="fa fa-times" aria-hidden="true"></i>
                    </a>
                  </div>
                </li>
              {{/each}}
            {{else}}
            <li class="list-group-item">No notes added here, if you feel like you need a note, click on the
              <span class=""><i class="fa fa-plus" aria-hidden="true"></i></span> sign at in the category name!</li>
            <li class="list-group-item"></li>
            {{/if}}
          </ul>
        </div>
        <div class="tab-pane fade" id="unclassified" role="tabpanel" aria-labelledby="unclassified-tab">
          <ul class="list-group list-group-flush">
            {{# if unclassified}}
              {{#each unclassified}}
                <li class="list-group-item">
                  <div class="container-fluid">
                    {{title}}
                    <a href="{{viewURL id}}" class="badge badge-light mx-1">
                      <i class="fa fa-eye" aria-hidden="true"></i>
                    </a>
                    <a class="badge badge-light mx-1" href="{{noteURL id}}">
                      <i class="fa fa-pen-square" aria-hidden="true"></i>
                    </a>
                    <a class="badge badge-light mx-1" href="{{deleteURL id}}">
                      <i class="fa fa-times" aria-hidden="true"></i>
                    </a>
                  </div>
                </li>
              {{/each}}
            {{else}}
            <li class="list-group-item">No notes added here, if you feel like you need a note, click on the
              <span class=""><i class="fa fa-plus" aria-hidden="true"></i></span> sign at in the category name!</li>
            <li class="list-group-item"></li>
            {{/if}}
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
